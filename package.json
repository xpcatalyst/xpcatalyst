{
  "name": "xpcatalyst",
  "private": true,
  "type": "module",
  "version": "0.0.2",
  "description": "Gain professional experience by collaborating on real-world projects",
  "author": "Jérôme Abel",
  "license": "CC BY-NC-SA 4.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:local": "cross-env SUPABASE_URL=http://localhost:54321 SUPABASE_KEY=your-local-key nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --coverage --ui",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:studio": "supabase studio",
    "db:new-migration": "supabase migration new",
    "db:push": "supabase db push",
    "db:generate-types": "supabase gen types typescript --local > types/supabase.ts",
    "db:setup": "pnpm db:start && pnpm db:reset && pnpm db:generate-types",
    "test:integration": "vitest run --config vitest.integration.config.ts"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "pnpm lint:fix"
  },
  "dependencies": {
    "@nuxt/eslint": "^0.5.7",
    "@nuxt/fonts": "^0.10.2",
    "@nuxt/icon": "^1.5.8",
    "@nuxt/image": "^1.8.1",
    "@nuxtjs/supabase": "^1.4.1",
    "@vueuse/core": "^11.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.452.0",
    "nuxt": "^3.13.2",
    "radix-vue": "^1.9.7",
    "shadcn-nuxt": "^0.10.4",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vue": "latest",
    "vue-router": "latest"
  },
  "packageManager": "pnpm@9.12.1",
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.1",
    "@iconify-json/ph": "^1.2.1",
    "@nuxt/test-utils": "^3.14.4",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@playwright/test": "^1.48.2",
    "@vitest/ui": "2.1.3",
    "@vue/devtools-api": "^7.5.4",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "playwright-core": "^1.48.2",
    "supabase": "^1.207.9",
    "typescript": "~5.5.4",
    "vitest": "^2.1.3"
  }
}
